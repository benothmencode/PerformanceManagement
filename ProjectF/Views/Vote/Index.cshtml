                                <tr>
                                    <td> @item.Title </td>
                                    <td>
                                        @item.Quantity
                                    </td>
                                    <td>
                                        <button type="button" class="btn btn-primary">Offer</button>
                                    </td>
                                </tr>
                                }
                               
                            </tbody>
                        </table>

